
    <div class="container">
      <h4 class="mt-3">List User</h4>
      <table class="table">
          <thead>
            <tr>
              <th scope="col">Name</th>
              <th scope="col">Email</th>
              <th scope="col">Password</th>
              <th scope="col">Edit</th>
              <th scope="col">Delete</th>
            </tr>
          </thead>
          <tbody>
              <%
              users.map(usr=>{%>
                  <tr>
                      <th scope="row"><%= usr.name %></th>
                      <th scope="row"><%= usr.email %></th>
                      <th scope="row"><%= usr.password %></th>
                      <td><a href="" class="btn btn-secondary">Edit</a></td>
                      <td>
                        <%-include('../parts/formDelete',{url:`/user/delete/${usr._id}`})%>
                      </td>
                  </tr>
                  <%})
              %>
          </tbody>
        </table>
  </div>
    </div>
    